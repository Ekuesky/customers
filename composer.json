{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/lumen-framework": "^8.3.1",
        "flipbox/lumen-generator": "^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.3",
        "laravel/helpers": "^1.4.1",
        "drewlabs/contracts": "^1.0",
        "drewlabs/core": "^1.0",
        "drewlabs/core-helpers": "^1.0",
        "drewlabs/database": "^v1.0",
        "drewlabs/http-client": "^1.0",
        "drewlabs/http": "^1.0",
        "drewlabs/uploaded-files": "^1.0",
        "drewlabs/server-authorized-clients": "^1.0",
        "drewlabs/filesystem": "^1.0",
        "drewlabs/psr7-stream": "^1.0",
        "drewlabs/lumen-vendor-publish": "^1.0",
        "drewlabs/helpers": "^1.0",
        "drewlabs/forms": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5.10",
        "drewlabs/code-generator": "^1.0",
        "drewlabs/component-generators": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-contracts.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-core-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-packages-database.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-laravel-http.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-http-client.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/php-server-authorized-clients.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-packages-uploaded-files.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-filesystem.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-psr7-stream.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/lumen-vendor-publish.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-packages-forms.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-mvc-components-generator.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:liksoft/drewlabs-php-code-generator.git"
        }
    ]
}
